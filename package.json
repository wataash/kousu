    "xmldom": "^0.5.0",
    "xpath": "0.0.32"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.15",
    "@types/jsdom": "^16.2.6",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.31",
    "@types/node-fetch": "^2.5.8",
