{
  "name": "kousu",
  "description": "ビーブレイクシステムズMA-EYES（webアプリ版）の作業実績（工数）入力を行う",
  "version": "1.0.0",
  "author": "Wataru Ashihara @wataash",
  "bin": {
    "kousu": "./bin/run"
  },
  "bugs": "https://github.com/wataash/kousu/issues",
  "dependencies": {
    "@oclif/command": "^1.8.16",
    "@oclif/config": "^1.18.3",
    "@oclif/plugin-help": "^5.1.12",
    "@xmldom/xmldom": "^0.8.2",
    "date-format": "^4.0.13",
    "jsdom": "^20.0.0",
    "log4js": "^6.6.1",
    "puppeteer": "^17.1.0",
    "tslib": "^2.4.0",
    "xpath": "0.0.32"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.10",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^18.7.14",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "eslint": "^8.23.0",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.2",
    "eslint-config-prettier": "^8.5.0",
    "globby": "^13.1.2",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/wataash/kousu",
  "keywords": [
    "oclif"
  ],
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "kousu",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "wataash/kousu",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && cp src/logger.js lib/ && oclif-dev manifest && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
